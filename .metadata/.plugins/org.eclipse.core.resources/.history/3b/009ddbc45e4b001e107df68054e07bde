
public class Main {

	public static void main(String[] args) {
		
		Salesman s1 = new Salesman("Jim", 1);
		Salesman s2 = new Salesman("Sam", 2);
		Salesman s3 = new Salesman("Bob", 3);
		Salesman s4 = new Salesman("John", 5);
		Salesman s5 = new Salesman("Steve", 1);
		Salesman[] dudes = {s1, s2, s3, s4, s5};
		Salesman topFive[] = new Salesman[5];
		float first = 0;
		
		for(int j = 0; j < topFive.length; j++ )
		{
			for(int i = 0; i < dudes.length; i++) 
			{
				if(dudes[i].inTopFive == false)
				{
					if(first < dudes[i].sales) 
					{
						first = dudes[i].sales;
						topFive[j] = dudes[i];
					}
				}	
			}
			for(int k = 0; k < dudes.length; k++)
			{
				if(first == dudes[k].sales)
				{
					dudes[i].inTopFive = true;
					first = 0;
				}
				
			}
			
			System.out.println(topFive[j].name);
			
		}
			
		

	}

}
